{
  "manifest_version": 3,
  "name": "Crypto Analyzer",
  "version": "1.0",
  "description": "Captures a screenshot of a crypto chart and sends it for AI analysis.",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "16": "icon.png",
        "32": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}